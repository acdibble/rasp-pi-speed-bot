{
  "name": "rasp-pi-speed-bot",
  "version": "0.0.1",
  "description": "",
  "main": "src",
  "scripts": {
    "dev": "nodemon -r dotenv/config --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start": "npm i && tsc && pm2 start pm2.json",
    "stop": "pm2 kill",
    "restart": "npm i && tsc && pm2 restart HotSpotlight",
    "test": "cross-env NODE_ENV=test nyc --all --reporter=text mocha 'test/**/*.test.js' --exit",
    "posttest": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acdibble/rasp-pi-speed-bot.git"
  },
  "author": "Andrew Dibble, Jean Paul Angelle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acdibble/rasp-pi-speed-bot/issues"
  },
  "homepage": "https://github.com/acdibble/rasp-pi-speed-bot#readme",
  "dependencies": {
    "dotenv": "^6.0.0",
    "fast-speedtest-api": "^0.3.1",
    "mathjs": "^5.0.4",
    "mongoose": "^5.2.6",
    "node-cron": "^1.2.1",
    "puppeteer": "^1.7.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/mathjs": "^4.4.1",
    "@types/mongoose": "^5.2.5",
    "@types/node-cron": "^1.2.0",
    "@types/puppeteer": "^1.6.0",
    "@types/request": "^2.47.1",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "nyc": "11.4.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
